{
    "name": "ocubom/twig-extra-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for custom extra Twig extensions",
    "keywords": [
        "symfony",
        "symfony-bundle",
        "ocubom",
        "twig",
        "extra",
        "bundle",
        "html",
        "minify",
        "compress",
        "svg"
    ],
    "homepage": "https://ocubom.github.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Oscar Cubo Medina",
            "homepage": "https://ocubom.github.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ocubom\\TwigExtraBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ocubom\\TwigExtraBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "symfony/config": "^5.4 || ^6.0 || ^7.0",
        "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
        "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
        "symfony/twig-bundle": "^5.4 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "ocubom/twig-html-extension": "^1.0 || ^2.0",
        "ocubom/twig-svg-extension": "^1.0 || ^2.0",
        "symfony/phpunit-bridge": "^6.1 || ^7.0",
        "symfony/webpack-encore-bundle": "^1.16 || ^2.0"
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix",
        "test": "./vendor/bin/simple-phpunit"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        },
        "sort-packages": true
    }
}
